-
    const backProps = {
        classes: '',
        title: '',
    }

mixin back(props = backProps, ...dataset)
    -
        const checkProps = {
            classes: props.classes ? props.classes : backProps.classes,
            title: props.title ? props.title : backProps.title,
        }
    .back
        button(data-smooth-scrolling="#2023")
            <svg width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M17 11H5" stroke="#F1F1F1"/>
            <path d="M9 7L5 11L9 15" stroke="#F1F1F1"/>
            </svg>
            span=checkProps.title